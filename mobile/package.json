{
  "name": "yieldrails-mobile",
  "version": "1.0.0",
  "description": "YieldRails Mobile Application - React Native app for DeFi yield optimization and cross-chain payments",
  "main": "index.js",
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace YieldRailsMobile.xcworkspace -scheme YieldRailsMobile -configuration Release -destination generic/platform=iOS -archivePath build/YieldRailsMobile archive",
    "clean": "react-native clean-project-auto",
    "pods": "cd ios && pod install"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-native-camera-roll/camera-roll": "^5.7.4",
    "@react-native-community/netinfo": "^11.2.1",
    "@react-native-firebase/app": "^18.7.3",
    "@react-native-firebase/messaging": "^18.7.3",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@reduxjs/toolkit": "^2.0.1",
    "react": "18.2.0",
    "react-native": "0.73.2",
    "react-native-biometrics": "^3.0.1",
    "react-native-chart-kit": "^6.12.0",
    "react-native-config": "^1.5.1",
    "react-native-device-info": "^10.12.0",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.14.1",
    "react-native-keychain": "^8.1.3",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-mmkv": "^2.11.0",
    "react-native-paper": "^5.11.6",
    "react-native-push-notification": "^8.1.1",
    "react-native-qrcode-scanner": "^1.5.5",
    "react-native-reanimated": "^3.6.2",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "^3.29.0",
    "react-native-svg": "^14.1.0",
    "react-native-vector-icons": "^10.0.3",
    "react-native-wallet-connect": "^1.8.0",
    "react-native-web3-wallet": "^1.1.2",
    "react-redux": "^9.0.4",
    "redux-persist": "^6.0.0",
    "redux-toolkit": "^1.1.2",
    "ethers": "^6.9.2",
    "axios": "^1.6.2",
    "react-hook-form": "^7.48.2",
    "date-fns": "^3.0.6",
    "react-native-dotenv": "^3.4.9"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.73.1",
    "@react-native/metro-config": "^0.73.3",
    "@react-native/typescript-config": "^0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4",
    "detox": "^20.13.5",
    "@testing-library/react-native": "^12.4.2",
    "@testing-library/jest-native": "^5.4.3"
  },
  "engines": {
    "node": ">=18"
  },
  "private": true,
  "keywords": [
    "react-native",
    "mobile",
    "defi",
    "yield",
    "crypto",
    "blockchain",
    "payments",
    "cross-chain"
  ],
  "author": "YieldRails Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yieldrails/yieldrails-mobile.git"
  },
  "bugs": {
    "url": "https://github.com/yieldrails/yieldrails-mobile/issues"
  },
  "homepage": "https://yieldrails.com"
}